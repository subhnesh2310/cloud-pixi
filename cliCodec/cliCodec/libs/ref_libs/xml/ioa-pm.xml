<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <pm xmlns="http://infinera.com/yang/ioa/pm">
    <real-time-pm-data>
      <real-time-pm>
        <resource/>
        <parameter/>
        <resource-type/>
        <AID/>
        <pm-unit/>
      </real-time-pm>
    </real-time-pm-data>
    <current-pm-data>
      <current-pm>
        <resource/>
        <parameter/>
        <period/>
        <monitoring-date-time/>
        <resource-type/>
        <AID/>
        <pm-unit/>
        <validity/>
      </current-pm>
    </current-pm-data>
    <history-pm-data>
      <history-pm>
        <resource/>
        <parameter/>
        <period/>
        <bin/>
        <monitoring-date-time/>
        <resource-type/>
        <AID/>
        <pm-unit/>
        <validity/>
      </history-pm>
    </history-pm-data>
    <pm-control>
      <pm-resource>
        <resource/>
        <resource-type/>
        <AID/>
        <pm-control-entry>
          <period/>
          <supported-parameters>
            <!-- # entries: 0..100 -->
          </supported-parameters>
          <data-supervision/>
          <tca-supervision/>
        </pm-control-entry>
      </pm-resource>
    </pm-control>
    <pm-profile>
      <pm-profile-entry>
        <resource-type/>
        <period/>
        <default-data-supervision/>
        <default-tca-supervision/>
        <pm-threshold-profile>
          <parameter/>
        </pm-threshold-profile>
      </pm-profile-entry>
    </pm-profile>
    <pm-catalog>
      <pm-parameter>
        <parameter/>
        <units/>
        <type/>
      </pm-parameter>
    </pm-catalog>
  </pm>
</data>
