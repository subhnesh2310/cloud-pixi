{
    "ne": {
        "system": {
            "protocols": {
                "grpc": {
                    "enabled": {
                        "type": {
                            "ptype": "bool"
                        },
                        "isList": false,
                        "description": "Enables/disables the gRPC management protocol.",
                        "mandatory": false,
                        "key": false,
                        "config": true,
                        "default": true,
                        "hardcode": false,
                        "namespace": null
                    },
                    "port": {
                        "type": {
                            "ptype": "int"
                        },
                        "isList": false,
                        "description": "The port which listens for gNMI access via gRPC.",
                        "mandatory": false,
                        "key": false,
                        "config": true,
                        "default": 50051,
                        "hardcode": false,
                        "namespace": null
                    },
                    "gnmi-get-encoding-granularity": {
                        "type": {
                            "ptype": "str",
                            "values": [
                                "per-path",
                                "per-object"
                            ]
                        },
                        "isList": false,
                        "description": "Allows to configure the granularity of data in gNMI Get responses, when encoded with JSON.",
                        "mandatory": false,
                        "key": false,
                        "config": true,
                        "default": "per-object",
                        "hardcode": false,
                        "namespace": null
                    }
                }
            },
            "namespace": {
                "prefix": null,
                "namespace": "http://openconfig.net/yang/system"
            }
        }
    }
}