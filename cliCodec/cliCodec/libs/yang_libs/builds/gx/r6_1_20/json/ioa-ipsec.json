{
    "module": {
        "@name": "ioa-ipsec",
        "@xmlns": "urn:ietf:params:xml:ns:yang:yin:1",
        "@xmlns:ioa-ipsec": "http://infinera.com/yang/ioa/ipsec",
        "@xmlns:ioa-common": "http://infinera.com/yang/ioa/common",
        "@xmlns:inet": "urn:ietf:params:xml:ns:yang:ietf-inet-types",
        "namespace": {
            "@uri": "http://infinera.com/yang/ioa/ipsec"
        },
        "prefix": {
            "@value": "ioa-ipsec"
        },
        "import": [
            {
                "@module": "ioa-common",
                "prefix": {
                    "@value": "ioa-common"
                }
            },
            {
                "@module": "ietf-inet-types",
                "prefix": {
                    "@value": "inet"
                }
            }
        ],
        "organization": {
            "text": "Infinera"
        },
        "contact": {
            "text": "Infinera Technical Support <techsupport@infinera.com>"
        },
        "description": {
            "text": "IOA YANG module that defines the data model for IPsec used across control and management plane\nsecurity applications supported on Infinera platforms."
        },
        "revision": [
            {
                "@date": "2022-10-31",
                "description": {
                    "text": "YANG Models for Infinera Open API (IOA) for release R6.0"
                }
            },
            {
                "@date": "2022-06-30",
                "description": {
                    "text": "YANG Models for Infinera Open API (IOA) for release R5.2"
                }
            },
            {
                "@date": "2022-02-28",
                "description": {
                    "text": "YANG Models for Infinera Open API (IOA) for release R5.1"
                }
            },
            {
                "@date": "2021-12-10",
                "description": {
                    "text": "YANG Models for Infinera Open API (IOA) for release R5.0"
                }
            }
        ],
        "typedef": [
            {
                "@name": "ike-sa-prf",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "hmac-sha2-256",
                            "value": {
                                "@value": "1"
                            }
                        },
                        {
                            "@name": "hmac-sha2-384",
                            "value": {
                                "@value": "2"
                            }
                        },
                        {
                            "@name": "hmac-sha2-512",
                            "value": {
                                "@value": "3"
                            }
                        },
                        {
                            "@name": "hmac-sha1",
                            "value": {
                                "@value": "4"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "All pseudo random functions used with the IKE SA."
                },
                "reference": {
                    "text": "https://www.iana.org/assignments/ikev2-parameters/ikev2-parameters.xhtml#ikev2-parameters-6"
                }
            },
            {
                "@name": "ike-sa-dh-group",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "dhe-2048",
                            "value": {
                                "@value": "1"
                            }
                        },
                        {
                            "@name": "dhe-3072",
                            "value": {
                                "@value": "2"
                            }
                        },
                        {
                            "@name": "dhe-4096",
                            "value": {
                                "@value": "3"
                            }
                        },
                        {
                            "@name": "dhe-6144",
                            "value": {
                                "@value": "4"
                            }
                        },
                        {
                            "@name": "dhe-8192",
                            "value": {
                                "@value": "5"
                            }
                        },
                        {
                            "@name": "ecp-256",
                            "value": {
                                "@value": "6"
                            }
                        },
                        {
                            "@name": "ecp-384",
                            "value": {
                                "@value": "7"
                            }
                        },
                        {
                            "@name": "ecp-521",
                            "value": {
                                "@value": "8"
                            }
                        },
                        {
                            "@name": "curve-25519",
                            "value": {
                                "@value": "9"
                            }
                        },
                        {
                            "@name": "curve-448",
                            "value": {
                                "@value": "10"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "All Diffie-Hellman groups used with the IKE SA."
                },
                "reference": {
                    "text": "https://www.iana.org/assignments/ikev2-parameters/ikev2-parameters.xhtml#ikev2-parameters-8"
                }
            },
            {
                "@name": "encryption-algorithm",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "null",
                            "value": {
                                "@value": "1"
                            },
                            "description": {
                                "text": "NULL encryption, used when authentication-only (no confidentiality) usage is desired"
                            }
                        },
                        {
                            "@name": "aes-gcm-8",
                            "value": {
                                "@value": "2"
                            },
                            "description": {
                                "text": "AES-GCM with 8-byte ICV."
                            }
                        },
                        {
                            "@name": "aes-gcm-12",
                            "value": {
                                "@value": "3"
                            },
                            "description": {
                                "text": "AES-GCM with 12-byte ICV."
                            }
                        },
                        {
                            "@name": "aes-gcm-16",
                            "value": {
                                "@value": "4"
                            },
                            "description": {
                                "text": "AES-GCM with 16-byte ICV."
                            }
                        },
                        {
                            "@name": "aes-ctr",
                            "value": {
                                "@value": "5"
                            },
                            "description": {
                                "text": "AES-CTR mode."
                            }
                        },
                        {
                            "@name": "aes-cbc",
                            "value": {
                                "@value": "6"
                            },
                            "description": {
                                "text": "AES-CBC mode."
                            }
                        },
                        {
                            "@name": "aes-ccm-8",
                            "value": {
                                "@value": "7"
                            },
                            "description": {
                                "text": "AES-CCM with 8-byte ICV."
                            }
                        },
                        {
                            "@name": "aes-ccm-12",
                            "value": {
                                "@value": "8"
                            },
                            "description": {
                                "text": "AES-CCM with 12-byte ICV."
                            }
                        },
                        {
                            "@name": "aes-ccm-16",
                            "value": {
                                "@value": "9"
                            },
                            "description": {
                                "text": "AES-CCM with 16-byte ICV."
                            }
                        },
                        {
                            "@name": "chacha20-poly1305",
                            "value": {
                                "@value": "10"
                            },
                            "description": {
                                "text": "ChaCha20/Poly1305 with 128 bit ICV"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "All encryption algorithms used with the Child SA and the IKE SA."
                },
                "reference": {
                    "text": "https://www.iana.org/assignments/ikev2-parameters/ikev2-parameters.xhtml#ikev2-parameters-5"
                }
            },
            {
                "@name": "integrity-algorithm",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "none",
                            "value": {
                                "@value": "0"
                            },
                            "description": {
                                "text": "Can be used only when the encryption algorithm uses authenticated encryption/AEAD (such as AES-GCM). Cannot be used with other non AEAD encryption algorithms such as AES-CTR or AES-CBC."
                            }
                        },
                        {
                            "@name": "hmac-sha2-256-128",
                            "value": {
                                "@value": "1"
                            }
                        },
                        {
                            "@name": "hmac-sha2-384-192",
                            "value": {
                                "@value": "2"
                            }
                        },
                        {
                            "@name": "hmac-sha2-512-256",
                            "value": {
                                "@value": "3"
                            }
                        },
                        {
                            "@name": "hmac-sha1-160",
                            "value": {
                                "@value": "4"
                            }
                        },
                        {
                            "@name": "hmac-sha1-96",
                            "value": {
                                "@value": "5"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "All Integrity algorithms used with the Child SA and the IKE SA."
                },
                "reference": {
                    "text": "https://www.iana.org/assignments/ikev2-parameters/ikev2-parameters.xhtml#ikev2-parameters-7"
                }
            },
            {
                "@name": "child-sa-dh-group",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "dhe-2048",
                            "value": {
                                "@value": "1"
                            }
                        },
                        {
                            "@name": "dhe-3072",
                            "value": {
                                "@value": "2"
                            }
                        },
                        {
                            "@name": "dhe-4096",
                            "value": {
                                "@value": "3"
                            }
                        },
                        {
                            "@name": "dhe-6144",
                            "value": {
                                "@value": "4"
                            }
                        },
                        {
                            "@name": "dhe-8192",
                            "value": {
                                "@value": "5"
                            }
                        },
                        {
                            "@name": "ecp-256",
                            "value": {
                                "@value": "6"
                            }
                        },
                        {
                            "@name": "ecp-384",
                            "value": {
                                "@value": "7"
                            }
                        },
                        {
                            "@name": "ecp-521",
                            "value": {
                                "@value": "8"
                            }
                        },
                        {
                            "@name": "curve-25519",
                            "value": {
                                "@value": "9"
                            }
                        },
                        {
                            "@name": "curve-448",
                            "value": {
                                "@value": "10"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "All Diffie-Hellman groups used with the Child SA."
                },
                "reference": {
                    "text": "https://www.iana.org/assignments/ikev2-parameters/ikev2-parameters.xhtml#ikev2-parameters-8"
                }
            },
            {
                "@name": "protocol-id",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "IKE",
                            "value": {
                                "@value": "1"
                            }
                        },
                        {
                            "@name": "ESP",
                            "value": {
                                "@value": "3"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "The protocol ID used in the IKE SA and IPsec SA protocol proposals."
                },
                "reference": {
                    "text": "Section 3.3 in RFC 7296."
                }
            },
            {
                "@name": "encryption-key-length",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "none",
                            "value": {
                                "@value": "0"
                            }
                        },
                        {
                            "@name": "key-length-128",
                            "value": {
                                "@value": "128"
                            }
                        },
                        {
                            "@name": "key-length-192",
                            "value": {
                                "@value": "192"
                            }
                        },
                        {
                            "@name": "key-length-256",
                            "value": {
                                "@value": "256"
                            }
                        }
                    ]
                },
                "units": {
                    "@name": "bits"
                },
                "description": {
                    "text": "The key length (in bits) for the encryption algorithm."
                }
            },
            {
                "@name": "start-port",
                "type": {
                    "@name": "union",
                    "type": [
                        {
                            "@name": "inet:port-number"
                        },
                        {
                            "@name": "enumeration",
                            "enum": [
                                {
                                    "@name": "all",
                                    "value": {
                                        "@value": "0"
                                    }
                                },
                                {
                                    "@name": "opaque",
                                    "value": {
                                        "@value": "255"
                                    }
                                }
                            ]
                        }
                    ]
                },
                "description": {
                    "text": "The values for the starting port"
                }
            }
        ],
        "grouping": [
            {
                "@name": "ipsec-sa-protocol-proposals",
                "list": {
                    "@name": "ipsec-sa-proposal",
                    "key": {
                        "@value": "number"
                    },
                    "leaf": [
                        {
                            "@name": "number",
                            "type": {
                                "@name": "uint8",
                                "range": {
                                    "@value": "1..max"
                                }
                            },
                            "description": {
                                "text": "The proposal number for the IPsec SA."
                            },
                            "reference": {
                                "text": "Section 3.3 in RFC 7296."
                            }
                        },
                        {
                            "@name": "protocol-id",
                            "type": {
                                "@name": "protocol-id"
                            },
                            "default": {
                                "@value": "ESP"
                            },
                            "config": {
                                "@value": "false"
                            },
                            "description": {
                                "text": "The protocol ID (type) for which the IPsec Child SA proposal applies to."
                            }
                        }
                    ],
                    "list": {
                        "@name": "encryption-algorithm",
                        "when": {
                            "@condition": "../../ipsec-protocol = 'ESP'"
                        },
                        "key": {
                            "@value": "algorithm key-length"
                        },
                        "leaf": [
                            {
                                "@name": "algorithm",
                                "type": {
                                    "@name": "encryption-algorithm"
                                },
                                "mandatory": {
                                    "@value": "true"
                                },
                                "description": {
                                    "text": "The encryption algorithm for the IPsec SA."
                                }
                            },
                            {
                                "@name": "key-length",
                                "type": {
                                    "@name": "encryption-key-length"
                                },
                                "mandatory": {
                                    "@value": "true"
                                },
                                "description": {
                                    "text": "The IPsec SA encryption algorithm key length."
                                }
                            }
                        ],
                        "description": {
                            "text": "A list of IPsec SA encryption algorithms\nadvertised to the far-end IKE peer."
                        }
                    },
                    "leaf-list": [
                        {
                            "@name": "integrity-algorithm",
                            "type": {
                                "@name": "integrity-algorithm"
                            },
                            "description": {
                                "text": "A list of IPsec SA integrity algorithms\nadvertised to the far-end IKE peer."
                            }
                        },
                        {
                            "@name": "dh-group",
                            "type": {
                                "@name": "child-sa-dh-group"
                            },
                            "min-elements": {
                                "@value": "1"
                            },
                            "description": {
                                "text": "A list of IPsec SA Diffie-Hellman groups\nadvertised to the far-end IKE peer. NOTE:\nThe 'min-elements' is 1, which means perfect\nforward secrecy (PFS) for IPsec Child SA is\nalways enabled."
                            }
                        }
                    ],
                    "description": {
                        "text": "A list of protocol proposals when negotiating the IPsec SA\nfor this SPD entry with the far-end IKE peer."
                    }
                }
            },
            {
                "@name": "ipsec-traffic-selector-attributes",
                "description": {
                    "text": "A collection of attributes associated with\nIPsec traffic selectors."
                },
                "leaf": [
                    {
                        "@name": "name",
                        "type": {
                            "@name": "ioa-common:object-name"
                        },
                        "description": {
                            "text": "A unique name to identify this IPsec traffic selector entry."
                        }
                    },
                    {
                        "@name": "next-layer-protocol",
                        "type": {
                            "@name": "union",
                            "type": [
                                {
                                    "@name": "uint8"
                                },
                                {
                                    "@name": "enumeration",
                                    "enum": [
                                        {
                                            "@name": "any",
                                            "value": {
                                                "@value": "0"
                                            }
                                        },
                                        {
                                            "@name": "opaque",
                                            "value": {
                                                "@value": "255"
                                            }
                                        }
                                    ]
                                }
                            ]
                        },
                        "default": {
                            "@value": "any"
                        },
                        "description": {
                            "text": "Indicates the inner protocol (upper layer), obtained\nfrom the IPv4 protocol or the IPv6 next header field."
                        },
                        "reference": {
                            "text": "Section 4.4.1.1, 4.4.1.2 in RFC 4301 and\nhttps://www.iana.org/assignments/protocol-numbers/protocol-numbers.xhtml"
                        }
                    }
                ],
                "uses": {
                    "@name": "ioa-common:aid",
                    "refine": {
                        "@target-node": "AID",
                        "description": {
                            "text": "A system-populated access identifier for this traffic selector entry."
                        }
                    }
                },
                "list": [
                    {
                        "@name": "local-subnet",
                        "key": {
                            "@value": "prefix"
                        },
                        "leaf": {
                            "@name": "prefix",
                            "type": {
                                "@name": "inet:ip-prefix"
                            }
                        },
                        "description": {
                            "text": "This is a list of ranges of IPv4/IPv6\naddresses (unicast, broadcast (IPv4 only))."
                        },
                        "reference": {
                            "text": "Section 4.4.1.1, 4.4.1.2 in RFC 4301"
                        }
                    },
                    {
                        "@name": "remote-subnet",
                        "key": {
                            "@value": "prefix"
                        },
                        "leaf": {
                            "@name": "prefix",
                            "type": {
                                "@name": "inet:ip-prefix"
                            }
                        },
                        "description": {
                            "text": "This is a list of ranges of IPv4/IPv6\naddresses (unicast, broadcast (IPv4 only))."
                        },
                        "reference": {
                            "text": "Section 4.4.1.1, 4.4.1.2 in RFC 4301"
                        }
                    },
                    {
                        "@name": "local-ports",
                        "key": {
                            "@value": "start stop"
                        },
                        "leaf": [
                            {
                                "@name": "start",
                                "type": {
                                    "@name": "start-port"
                                },
                                "description": {
                                    "text": "The port number where the range starts or a predefined value (all, opaque)."
                                }
                            },
                            {
                                "@name": "stop",
                                "type": {
                                    "@name": "inet:port-number"
                                },
                                "description": {
                                    "text": "The port number where the range ends."
                                }
                            }
                        ],
                        "description": {
                            "text": "A list of local ports ranges associated with this traffic selector."
                        },
                        "reference": {
                            "text": "Section 4.4.1.1, 4.4.1.2 in RFC 4301"
                        }
                    },
                    {
                        "@name": "remote-ports",
                        "key": {
                            "@value": "start stop"
                        },
                        "leaf": [
                            {
                                "@name": "start",
                                "type": {
                                    "@name": "start-port"
                                },
                                "description": {
                                    "text": "The port number where the range starts or a predefined value (all, opaque)."
                                }
                            },
                            {
                                "@name": "stop",
                                "type": {
                                    "@name": "inet:port-number"
                                },
                                "description": {
                                    "text": "The port number where the range ends"
                                }
                            }
                        ],
                        "description": {
                            "text": "A list of remote ports ranges associated with this traffic selector."
                        },
                        "reference": {
                            "text": "Section 4.4.1.1, 4.4.1.2 in RFC 4301"
                        }
                    }
                ]
            },
            {
                "@name": "ipsec-spd-entry",
                "description": {
                    "text": "A collection of attributes that are associated\nwith every IKEv2 SPD Entry."
                },
                "reference": {
                    "text": "IETF draft-ietf-i2nsf-sdn-ipsec-flow-protection,\n12ietf-i2nsf-ike@2020-10-30.yang module and\nSection 4.4.1.2 in RFC 4301."
                },
                "leaf": [
                    {
                        "@name": "name",
                        "type": {
                            "@name": "ioa-common:object-name",
                            "length": {
                                "@value": "1..32"
                            }
                        },
                        "description": {
                            "text": "A unique name to identify this SPD entry."
                        }
                    },
                    {
                        "@name": "priority",
                        "type": {
                            "@name": "uint8"
                        },
                        "mandatory": {
                            "@value": "true"
                        },
                        "description": {
                            "text": "A priority value for each SPD entry. This is\nused to give precedence to the SPD entries."
                        },
                        "reference": {
                            "text": "Section 4.4.1.2 in RFC 4301."
                        }
                    },
                    {
                        "@name": "description",
                        "type": {
                            "@name": "string",
                            "length": {
                                "@value": "0..128"
                            }
                        },
                        "description": {
                            "text": "User configurable label/description."
                        }
                    },
                    {
                        "@name": "action",
                        "type": {
                            "@name": "enumeration",
                            "enum": [
                                {
                                    "@name": "protect",
                                    "value": {
                                        "@value": "100"
                                    }
                                },
                                {
                                    "@name": "bypass",
                                    "value": {
                                        "@value": "200"
                                    }
                                },
                                {
                                    "@name": "discard",
                                    "value": {
                                        "@value": "300"
                                    }
                                }
                            ]
                        },
                        "default": {
                            "@value": "protect"
                        },
                        "description": {
                            "text": "Indicates the IPsec treatment given to the IP datagrams."
                        },
                        "reference": {
                            "text": "Section 4.4.1 ('Processing Choices') in RFC 4301."
                        }
                    },
                    {
                        "@name": "ipsec-protocol",
                        "when": {
                            "@condition": "../action = 'protect'"
                        },
                        "type": {
                            "@name": "enumeration",
                            "enum": {
                                "@name": "ESP",
                                "value": {
                                    "@value": "100"
                                },
                                "reference": {
                                    "text": "RFC 4301 and RFC 4303."
                                }
                            }
                        },
                        "default": {
                            "@value": "ESP"
                        },
                        "description": {
                            "text": "Indicates the use of ESP or AH IPsec protocols."
                        }
                    },
                    {
                        "@name": "mode",
                        "when": {
                            "@condition": "../action = 'protect'"
                        },
                        "type": {
                            "@name": "enumeration",
                            "enum": [
                                {
                                    "@name": "tunnel",
                                    "value": {
                                        "@value": "100"
                                    }
                                },
                                {
                                    "@name": "transport",
                                    "value": {
                                        "@value": "200"
                                    }
                                }
                            ]
                        },
                        "default": {
                            "@value": "tunnel"
                        },
                        "description": {
                            "text": "Indicates if the IPsec session should operate in\ntransport or tunnel mode."
                        }
                    },
                    {
                        "@name": "esn",
                        "type": {
                            "@name": "boolean"
                        },
                        "default": {
                            "@value": "true"
                        },
                        "description": {
                            "text": "Extended Sequence Number (ESN) support."
                        },
                        "reference": {
                            "text": "Section 2.2.1 in RFC 4303."
                        }
                    },
                    {
                        "@name": "anti-replay-window",
                        "when": {
