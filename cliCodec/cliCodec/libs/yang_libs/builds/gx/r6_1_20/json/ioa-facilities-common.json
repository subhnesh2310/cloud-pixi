                            "description": {
                                "text": "Terminal loopback."
                            }
                        }
                    ]
                },
                "description": {
                    "text": "Loopback type. Useful to debug on the fiber connection."
                }
            },
            {
                "@name": "service-mode",
                "type": {
                    "@name": "enumeration",
                    "enum": [
                        {
                            "@name": "none",
                            "value": {
                                "@value": "10"
                            }
                        },
                        {
                            "@name": "network-wrapper",
                            "value": {
                                "@value": "20"
                            },
                            "description": {
                                "text": "Map non-OTN signal into ODUs."
                            }
                        },
                        {
                            "@name": "adaptation",
                            "value": {
                                "@value": "30"
                            },
                            "description": {
                                "text": "Multiplexing scenarios."
                            }
                        },
                        {
                            "@name": "switching",
                            "value": {
                                "@value": "40"
                            },
                            "description": {
                                "text": "Map OTN signal (e.g. OTU) into ODUs."
                            }
                        },
                        {
                            "@name": "transport",
                            "value": {
                                "@value": "50"
                            },
                            "description": {
                                "text": "Transport OTN signal (e.g. OTU) into line side ODUs."
                            }
                        }
                    ]
                },
                "description": {
                    "text": "A custom type for legacy Infinera SM attribute."
                },
                "reference": {
                    "text": "Infinera OTN Virtualization System Requirements Document (SRD)."
                }
            },
            {
                "@name": "service-mode-qualifier",
                "type": {
                    "@name": "union",
                    "type": [
                        {
                            "@name": "enumeration",
                            "enum": [
                                {
                                    "@name": "none",
                                    "value": {
                                        "@value": "10"
                                    }
                                },
                                {
                                    "@name": "mux-demux",
                                    "value": {
                                        "@value": "20"
                                    }
                                },
                                {
                                    "@name": "nofec",
                                    "value": {
                                        "@value": "30"
                                    }
                                }
                            ]
                        },
                        {
                            "@name": "string",
                            "length": {
                                "@value": "0..32"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "A custom type for legacy Infinera SMQ attribute; string here represents the upper-layer protocol (e.g. ODUC4i)."
                },
                "reference": {
                    "text": "Infinera OTN Virtualization System Requirements Document (SRD)."
                }
            }
        ],
        "grouping": [
            {
                "@name": "facilities-common-attributes",
                "description": {
                    "text": "Common attributes that are applicable to all facility types."
                },
