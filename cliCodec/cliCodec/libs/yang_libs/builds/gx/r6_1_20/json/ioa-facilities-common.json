                            "@name": "instance-identifier"
                        },
                        "config": {
                            "@value": "false"
                        },
                        "max-elements": {
                            "@value": "20"
                        },
                        "description": {
                            "text": "An XPath reference to the children facilities."
                        }
                    }
                ],
