                "leaf": [
                    {
                        "@name": "auto-in-service-enabled",
                        "type": {
                            "@name": "boolean"
                        },
                        "default": {
                            "@value": "false"
                        },
                        "description": {
                            "text": "Auto-in-service switch for this facility."
                        }
                    },
                    {
                        "@name": "valid-signal-time",
                        "type": {
                            "@name": "ains-signal-time-interval",
                            "range": {
                                "@value": "1..7200"
                            }
                        },
                        "units": {
                            "@name": "minutes"
                        },
                        "default": {
                            "@value": "480"
                        },
                        "description": {
                            "text": "Configurable time that represents a detection of a valid signal.\nUsed for auto-in-service mechanism."
                        }
                    },
                    {
                        "@name": "remaining-valid-signal-time",
                        "type": {
                            "@name": "ains-signal-time-interval"
                        },
                        "units": {
                            "@name": "minutes"
                        },
                        "config": {
                            "@value": "false"
                        },
                        "description": {
                            "text": "Actual remaining time for this facility to be automatically enabled by the\nauto-in-service mechanism."
                        }
                    }
                ],
                "uses": {
                    "@name": "ioa-common:arc"
                }
            },
            {
                "@name": "test-signal",
                "leaf": [
                    {
                        "@name": "test-signal-type",
                        "type": {
                            "@name": "enumeration",
                            "enum": [
                                {
                                    "@name": "none",
                                    "value": {
                                        "@value": "0"
                                    },
                                    "description": {
                                        "text": "Indicates that test pattern generation is disabled."
                                    }
                                },
                                {
                                    "@name": "PRBS31Q",
                                    "value": {
                                        "@value": "10"
                                    },
                                    "description": {
                                        "text": "Defined in G.709 OPU PRBS with inverted PN31 quaternary."
                                    }
                                },
                                {
                                    "@name": "PRBS13Q",
                                    "value": {
                                        "@value": "20"
                                    },
                                    "description": {
                                        "text": "Defined in G.709 OPU PRBS with inverted PN13 quaternary."
                                    }
                                },
                                {
                                    "@name": "scrambled-idles",
                                    "value": {
                                        "@value": "30"
                                    },
                                    "description": {
                                        "text": "Idle frame defined in 802.3 Clause 82.2.10."
                                    }
                                },
                                {
                                    "@name": "PRBS9",
                                    "value": {
                                        "@value": "40"
                                    },
                                    "description": {
                                        "text": "Defined in G.709 OPU PRBS with non-inverted PN9."
                                    }
                                },
                                {
                                    "@name": "PRBS31",
                                    "value": {
                                        "@value": "50"
                                    },
                                    "description": {
                                        "text": "Defined in G.709 OPU PRBS with inverted PN31."
                                    }
                                }
                            ]
                        },
                        "default": {
                            "@value": "none"
                        },
                        "description": {
                            "text": "The type of test pattern that is injected."
                        }
                    },
                    {
                        "@name": "test-signal-direction",
                        "type": {
                            "@name": "prbs-direction"
                        },
                        "default": {
                            "@value": "ingress"
                        },
                        "description": {
                            "text": "The direction of the test signal."
                        }
                    },
                    {
                        "@name": "test-signal-monitoring",
                        "type": {
                            "@name": "boolean"
                        },
                        "default": {
                            "@value": "false"
                        },
                        "description": {
                            "text": "Monitor the incoming test signals for diagnostics."
                        }
                    }
                ]
            },
