            {
                "@name": "trail-trace-identifier-hex",
                "type": {
                    "@name": "string",
                    "pattern": {
                        "@value": "(0x(([0-9A-Fa-f])([0-9A-Fa-f]))*)?"
                    }
                },
                "description": {
                    "text": "Hex string with '0x' prefix."
                }
            },
            {
                "@name": "trail-trace-identifier-ascii",
                "type": {
                    "@name": "string",
                    "pattern": {
                        "@value": "[ -~]*"
                    }
                },
                "description": {
                    "text": "Restrict this to printable ASCII characters."
                }
            },
            {
                "@name": "tti-15",
                "type": {
                    "@name": "union",
                    "type": [
                        {
                            "@name": "trail-trace-identifier-hex",
                            "length": {
                                "@value": "0..32"
                            }
                        },
                        {
                            "@name": "trail-trace-identifier-ascii",
                            "length": {
                                "@value": "0..15"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "Hex string starting with '0x' or printable ASCII string with size 15."
                }
            },
            {
                "@name": "tti-32",
                "type": {
                    "@name": "union",
                    "type": [
                        {
                            "@name": "trail-trace-identifier-hex",
                            "length": {
                                "@value": "0..66"
                            }
                        },
                        {
                            "@name": "trail-trace-identifier-ascii",
                            "length": {
                                "@value": "0..32"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "Hex string starting with '0x' or printable ASCII string with size 32."
                }
            },
            {
                "@name": "tti-64",
                "type": {
                    "@name": "union",
                    "type": [
                        {
                            "@name": "trail-trace-identifier-hex",
                            "length": {
                                "@value": "0..130"
                            }
                        },
                        {
                            "@name": "trail-trace-identifier-ascii",
                            "length": {
                                "@value": "0..64"
                            }
                        }
                    ]
                },
                "description": {
                    "text": "Hex string starting with '0x' or printable ASCII string with size 64."
                }
            },
            {
